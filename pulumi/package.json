{
  "name": "nebula",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "exports": {
    "./src/*": "./dist/src/*.js",
    "./src/core/*": "./dist/src/core/*.js",
    "./src/components": "./dist/src/components/index.js",
    "./src/utils/*": "./dist/src/utils/*.js",
    "./src/components/k8s": "./dist/src/components/k8s/index.js"
  },
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/client-sso": "^3.716.0",
    "@aws-sdk/client-sts": "^3.738.0",
    "@aws-sdk/credential-providers": "^3.716.0",
    "@pulumi/aws": "^6.59.0",
    "@pulumi/awsx": "^2.15.0",
    "@pulumi/eks": "^2.6.3",
    "@pulumi/gcp": "^9.0.0",
    "@pulumi/kubernetes": "^4.16.0",
    "@pulumi/pulumi": "^3.197.0",
    "cdk8s": "2.69.57",
    "chalk": "^5.6.2",
    "constructs": "10.4.2",
    "deepmerge-ts": "^7.1.5",
    "js-yaml": "^4.1.0",
    "smol-toml": "^1.3.1",
    "yaml": "2.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
